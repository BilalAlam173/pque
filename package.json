{
  "name": "pque",
  "version": "1.0.0",
  "description": "Fast javascript library for priority queueu with variety of heap trees underneath",
  "main": "bin/bundle.js",
  "scripts": {
    "test": "npm run compile && jest",
    "test:watch": "npm run compile && jest --watch",
    "test:coverage": "npm run compile && jest --collect-coverage",
    "compile": "babel lib -d dist",
    "prebuild": "npm run compile",
    "build": "rollup dist/priorityQueue.js --file bin/bundle.js --format umd --name \"bin/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BilalAlam173/pque.git"
  },
  "files": [
    "bin/**"
  ],
  "keywords": [
    "priority",
    "queue",
    "heap",
    "queue",
    "fast",
    "priority",
    "queue",
    "priority",
    "queue",
    "with",
    "fibonacci",
    "heap"
  ],
  "author": "Bilal Alam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BilalAlam173/pque/issues"
  },
  "homepage": "https://github.com/BilalAlam173/pque#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-stage-3": "^7.8.3",
    "jest": "^26.6.3",
    "rollup": "^2.36.2"
  }
}
